<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
		PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
		"http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
	<!-- 配置mysql 驱动jar包路径.用了绝对路径 -->
	<!-- ATTENTION:这里需要把路径改为自己的.m2仓库 无法使用~符号！！！！！ -->
	<classPathEntry
			location="/Users/sreworks/.m2/repository/mysql/mysql-connector-java/5.1.44/mysql-connector-java-5.1.44.jar" />

	<context id="bcc_job_engine_mysql_tables" targetRuntime="MyBatis3">
		<plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin" />
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
		<plugin type="org.mybatis.generator.plugins.CaseInsensitiveLikePlugin" />
		<plugin type="org.mybatis.generator.plugins.ToStringPlugin" />
		<plugin type="org.mybatis.generator.plugins.RowBoundsPlugin" />
		<!--multiple insert, batch insert 批量插入方法 -->
		<!--<plugin type="com.itfsw.mybatis.generator.plugins.BatchInsertPlugin"/> -->

		<!-- 防止生成的代码中有很多注释，加入下面的配置控制 -->
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
			<property name="suppressDate" value="true" />
		</commentGenerator>

		<!-- 数据库连接 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
						connectionURL="jdbc:mysql://127.0.0.1:3306/warehouse?useUnicode=true&amp;characterEncoding=UTF-8"
						userId="root" password="root">
		</jdbcConnection>

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<javaModelGenerator targetPackage="com.alibaba.sreworks.warehouse.domain"
							targetProject="src/main/java/">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<sqlMapGenerator targetPackage="mybatis.mapper.tddl"
						 targetProject="src/main/resources/">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<javaClientGenerator type="XMLMAPPER"
							 targetPackage="com.alibaba.sreworks.warehouse.domain" targetProject="src/main/java/">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

		<table schema="warehouse" tableName="sw_domain"
			   domainObjectName="SwDomain" enableCountByExample="true"
			   enableUpdateByExample="true" enableDeleteByExample="true"
			   enableSelectByExample="true" selectByExampleQueryId="true">
			<generatedKey column="id" sqlStatement="MySql" identity="true" />
			<columnOverride column="=description" property="description" jdbcType="VARCHAR" />
		</table>
		<table schema="warehouse" tableName="sw_entity"
			   domainObjectName="SwEntity" enableCountByExample="true"
			   enableUpdateByExample="true" enableDeleteByExample="true"
			   enableSelectByExample="true" selectByExampleQueryId="true">
			<generatedKey column="id" sqlStatement="MySql" identity="true" />
			<columnOverride column="=description" property="description" jdbcType="VARCHAR" />
		</table>
		<table schema="warehouse" tableName="sw_entity_field"
			   domainObjectName="SwEntityField" enableCountByExample="true"
			   enableUpdateByExample="true" enableDeleteByExample="true"
			   enableSelectByExample="true" selectByExampleQueryId="true">
			<generatedKey column="id" sqlStatement="MySql" identity="true" />
			<columnOverride column="=description" property="description" jdbcType="VARCHAR" />
		</table>
		<table schema="warehouse" tableName="sw_model"
			   domainObjectName="SwModel" enableCountByExample="true"
			   enableUpdateByExample="true" enableDeleteByExample="true"
			   enableSelectByExample="true" selectByExampleQueryId="true">
			<generatedKey column="id" sqlStatement="MySql" identity="true" />
			<columnOverride column="=description" property="description" jdbcType="VARCHAR" />
		</table>
		<table schema="warehouse" tableName="sw_model_field"
			   domainObjectName="SwModelField" enableCountByExample="true"
			   enableUpdateByExample="true" enableDeleteByExample="true"
			   enableSelectByExample="true" selectByExampleQueryId="true">
			<generatedKey column="id" sqlStatement="MySql" identity="true" />
			<columnOverride column="=description" property="description" jdbcType="VARCHAR" />
		</table>
	</context>
</generatorConfiguration>

